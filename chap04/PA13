#define _CRT_SECURE_NO_WARNINGS
#include <math.h>
#include <stdio.h>

int main() {

	double length, x1, x2, y1, y2;

	printf("직선의 시작점 (x1, y1)? ");
	scanf("%lf %lf", &x1, &y1);

	printf("직선의 끝점 (x2, y2)? ");
	scanf("%lf %lf", &x2, &y2);

	length = (x2 - x1) * (x2 - x1) + (y2 - y1) * (y2 - y1);

	printf("직선의 길이: %lf", sqrt(length));

	return 0;
}
