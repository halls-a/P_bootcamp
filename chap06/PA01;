#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

int get_perimeter(int x, int y);

int main() {

	int x, y;

	printf("가로? ");
	scanf("%d", &x);

	printf("세로? ");
	scanf("%d", &y);

	get_perimeter(x, y);

	printf("직사각형의 둘레: %d", get_perimeter);

	return 0;
}

int get_perimeter(int x, int y) {

	int result = (x * 2) + (y * 2);

	return result;
}
